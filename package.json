{
  "name": "@m2g/algorithms",
  "version": "0.0.0",
  "description": "",
  "homepage": "https://github.com/M2G/algorithms",
  "bugs": {
    "url": "https://github.com/M2G/algorithms/issues"
  },
  "scripts": {
    "start2": "nodemon ./deleteGreater/index.js",
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node --experimental-modules --harmony-top-level-await build/index.js",
    "watch-node": "nodemon build/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "rm -rf ./build && tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect build/index.js",
    "test": "NODE_ENV=test jest --detectOpenHandles --verbose=false --forceExit --coverage",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "keywords": [
    "algorithms",
    "nodejs"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:algorithms/algorithms.git"
  },
  "type": "module",
  "author": "M2G",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-jest": "^26.6.3",
    "babel-preset-airbnb": "^5.0.0",
    "concurrently": "^5.3.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.4.0",
    "jest-cli": "^26.4.0",
    "jest-resolve": "26.4.0",
    "jest-watch-typeahead": "0.6.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "jest": "^26.4.0",
    "@types/node": "^14.11.2",
    "nodemon": "^2.0.2"
  }
}
